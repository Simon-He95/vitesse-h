{
  "name": "vitesse",
  "version": "0.0.1",
  "packageManager": "pnpm@7.33.7",
  "description": "vitesse",
  "author": "Simon He",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git + git@github.com:Simon-He95/vitesse.git"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/vitesse/issues"
  },
  "keywords": [
    "vitesse",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.6"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "build": "tsup ./src/index.ts --format cjs,esm --dts --clean --minify",
    "dev": "npm run build -- --watch src",
    "play": "pnpm run -C playground dev",
    "play:build": "pnpm run -C playground build",
    "format": "prettier --write --cache .",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest --dir test -u",
    "ptest": "pnpm run -C playground test -u",
    "preview": "pnpm run -C playground preview",
    "test:e2e": "cypress open",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --tag --push && git push origin --tags -f && npm publish"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.2",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-n": "^15.7.0",
    "lint-staged": "^13.3.0",
    "picocolors": "^1.0.1",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^6.7.0",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5",
    "vite": "^4.5.3",
    "vitest": "^0.13.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "pnpm exec tsx scripts/verifyCommit.ts $1"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
